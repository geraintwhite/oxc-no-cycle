{
    "parser": "@typescript-eslint/parser",
    "env": {
        "node": true
    },
    "settings": {
        "import/resolver": {
            "typescript": true,
            "node": true
        }
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-import"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:import/recommended",
        "plugin:import/typescript"
    ],
    "rules": {
        "import/no-cycle": "error"
    }
}
